{{component chartName
  data=data
  width=svgWidth
  height=svgHeight
  isIcon=isIcon
  hover=(perform handleHover)
  click=(perform handleClick)
  leave=(perform handleLeave)
}}

{{#if tooltipTarget}}
  {{yield (hash
    tooltip=(component 'simple-chart-tooltip'
      tetherTarget=tooltipTarget
      title=null
    )
  )}}
{{/if}}
