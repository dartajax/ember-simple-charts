<div class='ember-simple-charts-wrapper'>
  <header class="header">
    <h1>Ember Simple Charts</h1>
    <p>Some simple charts you can use in your email app.</p>
  </header>

  <div class="panel">
    {{simple-chart height=250 width=250 name='donut' data=donutData}}
  </div>
  <div class="panel">
    {{simple-chart height=250 width=250 name='pie' data=pieData}}
  </div>
  <div class="panel">
    {{simple-chart height=250 width=250 name='bar' data=barData}}
  </div>
  <div class="panel">
    {{simple-chart height=250 width=250 name='horz-bar' data=horzData}}
  </div>
  <div class="panel">
    {{simple-chart height=250 width=250 name='line' data=lineData}}
  </div>
  <div class="panel">
    {{simple-chart height=250 width=250 name='stacked-bar' data=stackedData}}
  </div>
  <div class="panel">
    {{simple-chart height=250 width=250 name='legend' data=legendData}}
  </div>

  <footer class="footer"></footer>
</div>
