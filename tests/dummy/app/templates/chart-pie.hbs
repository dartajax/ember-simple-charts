<header class="header">
  <h1>Ember Simple Charts</h1>
  <h2>Pie Chart</h2>
</header>

<div class='detail-chart'>
  <div class='icon-example example'>
    {{simple-chart isIcon=true name='pie' data=chartData.pie}}
  </div>

  <div class='medium-example example'>
    {{simple-chart name='pie' data=chartData.pie}}
  </div>

  <div class='large-example example'>
    {{simple-chart name='pie' data=chartData.pie}}
  </div>
</div>

<footer class="footer"></footer>
